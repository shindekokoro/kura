{
  "name": "kura",
  "description": "Compare the interest and principle payments across different loan payments. Do you pay more towards interest on your lower rate loan?",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint client/src --ext js,jsx --report-unused-disable-directives --max-warnings 10",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "readme": "node node_modules/git-a-readme/index.js",
    "lint-fix": "eslint client/src --ext js,jsx --report-unused-disable-directives --max-warnings 10 --fix",
    "fix": "yarn format && yarn lint:fix",
    "format": "prettier --write 'client/src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'client/src/**/*.{js,jsx,ts,tsx}'",
    "fix:eslint": "eslint client/src --ext .js,.ts,.jsx,.tsx --fix",
    "fix:prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shindekokoro/kura.git"
  },
  "author": "Brian Whisler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shindekokoro/kura/issues"
  },
  "homepage": "https://github.com/shindekokoro/kura#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "git-a-readme": "^1.2.5",
    "prettier": "^3.2.4",
    "vite": "^5.0.8"
  }
}
